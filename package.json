{
  "name": "electron-vue-typescript-boilerplate",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "prestart": "trash dist && webpack --config webpack.dev.main.js",
    "start": "npm-run-all -p start:*",
    "start:main": "webpack --config webpack.dev.main.js --watch",
    "start:renderer": "webpack-dev-server --config webpack.dev.renderer.js",
    "start:electron": "electron ./dist/main.js",
    "prebuild": "trash dist && copyfiles package.json dist && cd dist && npm install --production",
    "build": "webpack --config webpack.prod.js && node script/package.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "copyfiles": "^2.0.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.4",
    "electron-packager": "^10.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.0.3",
    "scss-loader": "0.0.1",
    "trash-cli": "^1.4.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.9.2",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "vue": "^2.5.16"
  }
}
